{
	"parser": "esprima",
	"globals": {
		"console": flase,
		"document": false,
		"module": false,
		"process": false,
		"require": false,
		"window": false
	},
	"rules": {
		"brace-style": [2, "1tbs", { "allowSingleLine": true }],
		"camelcase": 2,
		"comma-dangle": [2, "always-multiline"],
		"dot-location": [2, "property"],
		"eqeqeq": 2,
		"func-style": [2, "declaration"],
		"new-cap": [2, {capIsNewExceptions: ["Snap"]}],
		"newline-after-var": 2,
		"no-console": 0,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty": 2,
		"no-extend-native": 2,
		"no-fallthrough": 2,
		"no-floating-decimal": 2,
		"no-implied-eval": 2,
		"no-irregular-whitespace": 2,
		"no-lone-blocks": 2,
		"indent": [2, 2, {SwitchCase: 1}],
		"no-loop-func": 2,
		"max-nested-callbacks": [2, 3],
		"new-parens": 2,
		"no-array-constructor": 2,
		"no-multiple-empty-lines": 2,
		"no-nested-ternary": 2,
		"no-new-object": 2,
		"no-spaced-func": 2,
		"no-mixed-requires": [2, true],
		"no-mixed-spaces-and-tabs": 2,
		"no-multi-spaces": 2,
		"space-return-throw-case": 2,
		"no-multi-str": 2,
		"no-native-reassign": 2,
		"no-negated-in-lhs": 2,
		"no-new": 2,
		"no-new-func": 2,
		"no-new-require": 2,
		"no-new-wrappers": 2,
		"no-obj-calls": 2,
		"space-infix-ops": 2,
		"no-param-reassign": 2,
		"no-path-concat": 2,
		"no-redeclare": 2,
		"spaced-comment": [2, "always"],
		"max-len": [2, 120, 2],
		"no-return-assign": 2,
		"no-script-url": 2,
		"array-bracket-spacing": [2, "never"],
		"space-in-parens": [2, "never"],
		"no-throw-literal": 2,
		"no-underscore-dangle": 2,
		"object-curly-spacing": [2, "never"],
		"space-before-function-paren": [2, "never"],
		"no-unreachable": 2,
		"no-unused-vars": 2,
		"no-use-before-define": 2,
		"no-warning-comments": [1, { "terms": ["todo", "fixme", "debug"], "location": "start" }],
		"no-with": 2,
		"one-var": [2, "never"],
		"padded-blocks": [2, "never"],
		"quotes": [2, "single"],
		"radix": 2,
		"space-before-blocks": 2,
		"strict": [2, "never"],
		"use-isnan": 2,
		"valid-jsdoc": 2,
		"valid-typeof": 2,
		"vars-on-top": 2
	}
}